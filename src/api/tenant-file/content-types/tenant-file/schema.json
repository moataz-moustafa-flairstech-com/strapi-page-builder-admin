  "attributes": {
    "file_document_id": {
      "type": "string",
      "required": true,
      "description": "Upload plugin document id (plugin::upload.file document id)"
    },
    "tenant_id": {
      "type": "string",
      "default": "tenant24",
      "pluginOptions": {
        "content-manager": {
          "visible": true
        }
      }
    }
  }
